{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/frontend/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/app",
      "dest": "/main.py"
    },
    {
      "src": "/",
      "dest": "/main.py"
    }
  ],
  "env": {
    "FRONTEND_URL": "http://localhost:3000",
    "ENVIRONMENT": "production"
  }
}
